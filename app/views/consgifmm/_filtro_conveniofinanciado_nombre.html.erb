<% col = Cor1440Gen::Proyectofinanciero.where(
  'id IN (SELECT proyectofinanciero_id FROM consgifmm)').order(:nombre) %>
<% sel = [] %>
<% if params && params[:filtro] && 
    params[:filtro][:busconveniofinanciado_nombre] %>
  <% sel = params[:filtro][:busconveniofinanciado_nombre] %>
<% end %>
<%= f.input :busconveniofinanciado_nombre,
  collection: col,
  include_blank: true,
  label_method: :presenta_nombre,
  value_method: :id,
  label: false,
  selected: sel,
  input_html: {
    class: 'chosen-select',
    multiple: true,
    'data-enviarautomatico' => ''
  },
  wrapper_html: {
    style: 'width: 20em'
  }
%>
