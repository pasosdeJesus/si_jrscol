<datalist id='fuente-lugar'></datalist>
<datalist-info for='fuente-lugar'></datalist>
<%= f.simple_fields_for :migracion, f.object.migracion.order(:id) do |migracion| %>
  <%= render 'migracion_campos', f: migracion %>
<% end %>
<div class="links">
  <%= link_to_add_association 'Añadir Migración', f, :migracion, {
    partial: 'migracion_campos',
    class: 'btn btn-sm btn-primary',
    "data-ajax" => File.join(
      Rails.configuration.relative_url_root,
      "migraciones/nuevo"
    ), 
    "data-ajaxdata" => "caso_id" }
  %>
  </div>
